<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ReTurn - My ASP.NET Application</title>
    <link href="~/Content/Account.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="https://cdn.datatables.net/1.13.7/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    <nav class="navbar navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">ReTurn</a>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">
                <div class="collapse navbar-collapse" id="myNavbar">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="/Home/Index">Admissions</a></li>
                        <li><a href="/Home/Admissions">Admissions</a></li>
                        <li><a href="/Home/ContactUs">Contact Us</a></li>
                        <li><a href="/Home/AboutUs">About Us</a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="/Account/Login"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>

                    </ul>
                </div>

            </div>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()

        <footer>
            <div class="footer container">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>

            </div>
        </footer>
    </div>
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/Admin.js"></script>
    <script src="https://cdn.datatables.net/1.13.7/js/jquery.dataTables.min.js"> </script>
    <script>
        new DataTable('.table');    
    </script>
</body>
</html>